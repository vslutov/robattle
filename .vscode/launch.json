    "version": "0.2.0",
    "configurations": [
        {
            "type": "dascript",
            "request": "launch",
            "name": "daScript: launch",
            "program": "${config:dascript.compiler} robattle_main.das",
            "cwd": "${workspaceFolder}/projects/robattle",
            "connectTimeout": 150
        },
        {
            "type": "dascript",
            "request": "attach",
            "name": "daScript: attach",
            "port": 10000,
            "connectTimeout": 150
        },
        {
            "name": "cpp: launch",
            "type": "cppvsdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}/projects/robattle",
            "program": "D:/tools/robattle/cmake_tmp/Debug/dasbox.exe",
            "args": [
                "robattle_main.das",
            ],
        },
    ]
}